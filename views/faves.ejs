<% faves.forEach((movie) => {%>
    <li>
        <a href='http://localhost:3000/movie/<%= movie.imdbid %>'><%= movie.title %></a>

        <form action="/movie/fave/delete/<%= movie.imdbid %>/?_method=DELETE" method="POST">
            <input type="submit" value="Delete Movie">
        </form>
    </li>
<% }) %>